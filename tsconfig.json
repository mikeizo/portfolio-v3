{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "paths": {
    "three/webgpu": ["node_modules/three/build/three.webgpu.js"],
    "three/tsl": ["node_modules/three/build/three.tsl.js"],
  }
}